{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Mailer service",
    "description": "A simple API to get fibonacci series"
  },
  "schemes": [
    "https"
  ],
  "host": "",
  "basePath": "/",
  "paths": {
    "/mail/{end}": {
      "get": {
        "summary": "To send mails",
        "description": "Sends mails",
        "parameters": [
          {
          "name": "end",
          "in": "path",
          "description": "End of series",
          "required": true,
          "type": "integer"
        }
        ],
        "responses": {
          "200": {
            "description": "Sending mail is successful"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal server error"
          }
        },
        "x-amazon-apigateway-integration": {
          "credentials": "arn:aws:iam::836567955187:role/Intaglio_lambda_ApiGateWay_Role",
          "httpMethod": "post",
          "type": "aws",
          "uri": "arn:aws:apigateway:eu-west-1:lambda:path/2015-03-31/functions/arn:aws:lambda:eu-west-1:836567955187:function:test-node:PROD/invocations",
          "requestTemplates": {
            "application/json": "{\n \"end\": \"$input.params('end')\"\n}"
          },
          "responses": {
            "default": {
              "statusCode": 200
            },
            ".*BadRequest.*": {
              "statusCode": 400
            },
            ".*InternalServerError.*": {
              "statusCode": 500
            }
          }
        }
      }
    }
  },
  "definitions": {
    "postBody": {
      "type": "string",
      "description": "end of the series"
    }
  }
}